{
    "name": "taxusorg/xunsearch-laravel",
    "description": "Laravel XunSearch engine.",
    "keywords": [
        "xunsearch", "scout", "Laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Taxusorg",
            "email": "taxus@taxus.org"
        }
    ],
    "require": {
        "php": ">=7.1|>=8.0",
        "laravel/scout": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "hightman/xunsearch": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Taxusorg\\XunSearchLaravel\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "vlucas/phpdotenv": "^2.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Taxusorg\\XunSearchLaravel\\XunSearchServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "v4.0.x-dev"
        }
    }
}
